{
  "name": "wyrihaximus/test-utilities",
  "description": "Test utilities for api-clients packages",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "composer/composer": "^1.8",
    "ergebnis/composer-normalize": "^2.0",
    "icanhazstring/composer-unused": "^0.7",
    "infection/infection": "^0.15.3",
    "jakub-onderka/php-console-highlighter": "^0.4",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "maglnet/composer-require-checker": "^2.0",
    "nunomaduro/collision": "^3.0",
    "phpstan/phpstan": "^0.12.3",
    "phpunit/phpunit": "^9.0",
    "psalm/plugin-phpunit": "^0.9",
    "squizlabs/php_codesniffer": "^3.4.2",
    "thecodingmachine/safe": "^1.0",
    "vimeo/psalm": "^3.4",
    "wyrihaximus/coding-standard": "^1.0.0-alpha1",
    "wyrihaximus/phpstan-rules-wrapper": "^1.2"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "extra": {
    "unused": [
      "php",
      "composer/composer",
      "friendsofphp/php-cs-fixer",
      "icanhazstring/composer-unused",
      "infection/infection",
      "jakub-onderka/php-console-highlighter",
      "jakub-onderka/php-parallel-lint",
      "maglnet/composer-require-checker",
      "nunomaduro/collision",
      "phpstan/phpstan",
      "phpunit/phpunit",
      "psalm/plugin-phpunit",
      "slevomat/coding-standard",
      "vimeo/psalm",
      "wyrihaximus/phpstan-rules-wrapper"
    ]
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\TestUtilities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\TestUtilities\\": "tests/"
    },
    "files": [
      "tests/bootstrap.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
